    "@babel/runtime": "^7.11.2",
    "@types/node": "^16.0.0",
    "babel-loader": "^8.1.0",
    "core-js": "3",
    "prettier": "^2.0.5",
    "ts-loader": "^7.0.1",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^5.7.3"
